{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"linter": {
		"rules": {
			"suspicious": {
				"noExplicitAny": "warn",
				"noEmptyInterface": "off"
			},
			"style": {
				"noNonNullAssertion": "warn",
				"useImportType": "off"
			},
			"complexity": {
				"noUselessConstructor": "warn"
			}
		}
	},
	"javascript": {
		"parser": {
			"unsafeParameterDecoratorsEnabled": true
		}
	}
}
